<%= form_for(@registration_information) do |f| %>
  <% if @registration_information.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@registration_information.errors.count, "error") %> prohibited this registration_information from being saved:</h2>

      <ul>
      <% @registration_information.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.text_field :password %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :age %><br />
    <%= f.text_field :age %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :anti_bot_check %><br />
    <%= f.text_field :anti_bot_check %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
